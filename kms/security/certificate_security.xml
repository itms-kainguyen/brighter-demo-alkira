<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_category_certificate">
            <field name="name">KMS</field>
            <field name="description">Helps you manage certificate</field>
            <field name="sequence">12</field>
        </record>
    </data>

    <record id="group_certificate_user" model="res.groups">
        <field name="name">Knowledge Officer</field>
        <field name="category_id" ref="module_category_certificate"/>
    </record>

    <record id="base.default_user" model="res.users">
        <field name="groups_id" eval="[(4,ref('kms.group_certificate_user'))]"/>
    </record>

</odoo>