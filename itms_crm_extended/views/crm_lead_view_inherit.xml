<?xml version="1.0" encoding="UTF-8"?>
  <odoo>
  <data>
    <record id="qlm_reorder_customer_view" model="ir.ui.view">
      <field name="name">crm.lead.form.inherit</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.crm_lead_view_form"/>
      <field name="arch" type="xml">
        <page name="internal_notes" position="before">
          <page name="registration" string="Registration Enquiry" >
              <group>
                  <group string="Information" >
                      <field name="entity_type" />
                      <field name="trading_name" />
                      <field name="abn" />
                      <field name="company_registered_address" />
                      <field name="company_website" />
                      <field name="company_instagram_handle" />
                      <field name="best_contact_number" />
                      <field name="best_contact_email" />
                  </group>
                  <group string="Operating hours for product delivery address">
                      <field name="delivery_hours_1" string="Monday" />
                      <field name="delivery_hours_2" string="Tuesday"/>
                      <field name="delivery_hours_3" string="Wednesday"/>
                      <field name="delivery_hours_4" string="Thursday"/>
                      <field name="delivery_hours_5" string="Friday"/>
                      <field name="delivery_hours_6" string="Saturday"/>
                      <field name="delivery_hours_7" string="Sunday"/>
                  </group>
                  <group string="Key Practitioner" >
                      <field name="practitioner_name" />
                      <field name="healthcare_qualifications" />
                      <field name="ahpra_details"/>
                      <field name="owner_employee"/>
                  </group>
                  <group string="" >
                      <field name="insurance"/>
                      <field name="key_practitioner_years_of_practice"/>
                      <field name="scope_of_practice"/>
                  </group>
                  <group string="Authorising Prescriber">
                      <field name="current_prescribing_service"/>
                      <field name="current_authorising_practitioner_name"/>
                      <field name="authorising_practice_clinic_name"/>
                  </group>
                  <group string="Product Requirements">
                      <field name="current_scheduled_product_accounts"/>
                      <field name="estimated_annual_scheduled_product_purchase_value"/>
                  </group>
              </group>
          </page>
        </page>
      </field>
    </record>

  </data>
</odoo>