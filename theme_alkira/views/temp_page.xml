<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="temp_page" model="website.page">
            <field name="name">Temp page</field>
            <field name="website_published">True</field>
            <field name="url">/temp</field>
            <field name="type">qweb</field>
            <field name="key">theme_alkira.temp_page</field>
            <field name="arch" type="xml">
                <t t-name="theme_alkira.temp_page_template">
                    <t t-call="website.layout">
                        <div id="wrap" class="container">
                            <div class="container">
                                Hello Temp page
                            </div>
                        </div>
                    </t>
                </t>
            </field>
        </record>
    </data>

</odoo>
